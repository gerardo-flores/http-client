{
    "family": "outage-http-client",
    "containerDefinitions": [
        {
            "image": "991821860035.dkr.ecr.us-east-1.amazonaws.com/outage-http-client",
            "name": "outage-http-client",
            "cpu": 10,
            "memory": 1024,
            "essential": true,
             "logConfiguration": {
       					 "logDriver": "awslogs",
       					 "options": {
         								 "awslogs-group": "dev-outage-http-client-log",
          								"awslogs-region": "us-east-1"
        							}
      					},
            "portMappings": [
                {
                    "containerPort":8900,
                    "hostPort": 8900
                }
            ],
             "environment" : [
   				   { "name": "http_proxy","value": "http://c1184651648.saasprotection.com:8080"},
   				   {"name": "https_proxy","value": "https://c1184651648.saasprotection.com:8080"}
    			
			]
        }
    ]
}